{
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "indent": ["error", 4],
        "react/jsx-indent": 4,
        "react/jsx-indent-props":4
    }
}